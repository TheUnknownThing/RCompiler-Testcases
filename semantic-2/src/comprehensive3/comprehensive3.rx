// Sieve of Eratosthenes for prime generation
fn sieveOfEratosthenes(limit: usize) -> i32 {
    let mut is_prime: [i32; 2000] = [0; 2000];
    let mut prime_count = 0;

    // Initialize all numbers as prime
    let mut i = 2;
    while (i < limit) {
        is_prime[i] = 1;
        i += 1;
    }

    // Sieve process
    i = 2;
    while (i * i < limit) {
        if (is_prime[i] == 1) {
            // Mark multiples as not prime
            let mut j = i * i;
            while (j < limit) {
                is_prime[j] = 0;
                j += i;
            }
        }
        i += 1;
    }

    // Count primes
    i = 2;
    while (i < limit) {
        if (is_prime[i] == 1) {
            prime_count += 1;
        }
        i += 1;
    }

    return prime_count;
}

// Perform multiple GCD operations using different algorithms
fn performGCDOperations(limit: i32) -> i32 {
    let mut gcd_sum = 0;
    let mut i = 1;

    while (i <= limit) {
        let mut j = 1;
        while (j <= limit) {
            // Calculate GCD using Euclidean algorithm
            gcd_sum += euclideanGCD(i * 13 + 7, j * 17 + 11);

            // Calculate GCD using Stein's algorithm (binary GCD)
            gcd_sum += binaryGCD(i * 19 + 3, j * 23 + 5);

            j += 10; // Skip some iterations for performance
        }
        i += 10;
    }

    return gcd_sum % 10000;
}

// Euclidean GCD algorithm
fn euclideanGCD(a: i32, b: i32) -> i32 {
    let mut x = a;
    let mut y = b;

    while (y != 0) {
        let temp = y;
        y = x % y;
        x = temp;
    }

    return x;
}

// Binary GCD algorithm (Stein's algorithm)
fn binaryGCD(a: i32, b: i32) -> i32 {
    let mut x = a;
    let mut y = b;
    let mut shift = 0;

    if (x == 0) {
        return y;
    }
    if (y == 0) {
        return x;
    }

    // Factor out common powers of 2
    while (((x | y) & 1) == 0) {
        shift += 1;
        x >>= 1;
        y >>= 1;
    }

    // Remove factors of 2 from x
    while ((x & 1) == 0) {
        x >>= 1;
    }

    while (y != 0) {
        // Remove factors of 2 from y
        while ((y & 1) == 0) {
            y >>= 1;
        }

        // Ensure x <= y
        if (x > y) {
            let temp = x;
            x = y;
            y = temp;
        }

        y = y - x;
    }

    return x << shift;
}

// Modular exponentiation operations
fn performModularExponentiation() -> i32 {
    let mut result_sum = 0;
    let mut base = 2;

    while (base <= 10) {
        let mut exponent = 1;
        while (exponent <= 20) {
            let mut modulus = 97; // Use prime modulus
            let mod_result = modularPower(base, exponent, modulus);
            result_sum = (result_sum + mod_result) % 10000;

            exponent += 1;
        }
        base += 1;
    }

    return result_sum;
}

// Fast modular exponentiation using binary method
fn modularPower(base: i32, exponent: i32, modulus: i32) -> i32 {
    let mut result = 1;
    let mut current_base = base % modulus;
    let mut current_exp = exponent;

    while (current_exp > 0) {
        if (current_exp % 2 == 1) {
            result = (result * current_base) % modulus;
        }

        current_exp >>= 1;
        current_base = (current_base * current_base) % modulus;
    }

    return result;
}

// Calculate Euler's totient function for numbers 1 to n
fn calculateTotientSum(n: i32) -> i32 {
    let mut totient_sum = 0;
    let mut i = 1;

    while (i <= n) {
        totient_sum += eulerTotient(i);
        i += 1;
    }

    return totient_sum % 10000;
}

// Euler's totient function φ(n)
fn eulerTotient(n: i32) -> i32 {
    let mut result = n;
    let mut num = n;
    let mut p = 2;

    // Check for factor 2
    if (num % 2 == 0) {
        while (num % 2 == 0) {
            num /= 2;
        }
        result -= result / 2;
    }

    // Check for odd factors
    p = 3;
    while (p * p <= num) {
        if (num % p == 0) {
            while (num % p == 0) {
                num /= p;
            }
            result -= result / p;
        }
        p += 2;
    }

    // If num is still greater than 1, then it's a prime
    if (num > 1) {
        result -= result / num;
    }

    return result;
}

// Find perfect numbers up to limit
fn findPerfectNumbers(limit: i32) -> i32 {
    let mut perfect_count = 0;
    let mut num = 6; // First perfect number

    while (num <= limit) {
        if (isPerfectNumber(num) == 1) {
            perfect_count += 1;
        }
        num += 1;
    }

    return perfect_count;
}

// Check if number is perfect (sum of proper divisors equals the number)
fn isPerfectNumber(n: i32) -> i32 {
    let mut divisor_sum = 1; // 1 is always a proper divisor
    let mut i = 2;

    while (i * i <= n) {
        if (n % i == 0) {
            divisor_sum += i;
            if (i != n / i) {
                divisor_sum += n / i;
            }
        }
        i += 1;
    }

    return (divisor_sum == n) as i32;
}

