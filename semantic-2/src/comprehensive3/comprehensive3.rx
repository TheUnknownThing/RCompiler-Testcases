    initializeMatrixB(&mut matrix_b, size);

    // Matrix multiplication
    multiplyMatrices(&matrix_a, &matrix_b, &mut result_matrix, size);

    // Calculate matrix trace
    let trace = calculateMatrixTrace(&result_matrix, size);

    // Calculate matrix determinant (using cofactor expansion for small submatrix)
    let mut small_matrix: [i32; 16] = [0; 16]; // 4x4 submatrix
    extractSubmatrix(&result_matrix, &mut small_matrix, size, 4);
    let determinant = calculateDeterminant4x4(small_matrix);

    return (trace + determinant) % 10000;
}

// Initialize matrix A with specific pattern
fn initializeMatrixA(matrix: &mut [i32; 625], size: usize) {
    let mut i = 0;
    while (i < size) {
        let mut j = 0;
        while (j < size) {
            matrix[i * size + j] = (i + j + 1) as i32 % 10;
            j += 1;
        }
        i += 1;
    }
}

// Initialize matrix B with different pattern
fn initializeMatrixB(matrix: &mut [i32; 625], size: usize) {
    let mut i = 0;
    while (i < size) {
        let mut j = 0;
        while (j < size) {
            matrix[i * size + j] = (i * j + 1) as i32 % 10;
            j += 1;
        }
        i += 1;
    }
}

// Matrix multiplication optimized for cache locality
fn multiplyMatrices(a: &[i32; 625], b: &[i32; 625], result: &mut [i32; 625], size: usize) {
    // Initialize result matrix
    let mut i = 0;
    while (i < size) {
        let mut j = 0;
        while (j < size) {
            result[i * size + j] = 0;
            j += 1;
        }
        i += 1;
    }

    // Blocked matrix multiplication for better cache performance
    let block_size = 5;
    let mut i_block = 0;

    while (i_block < size) {
        let mut j_block = 0;
        while (j_block < size) {
            let mut k_block = 0;
            while (k_block < size) {
                // Process block
                i = i_block;
                let mut i_end = i_block + block_size;
                if (i_end > size) {
                    i_end = size;
                }

                while (i < i_end) {
                    let mut j = j_block;
                    let mut j_end = j_block + block_size;
                    if (j_end > size) {
                        j_end = size;
                    }

                    while (j < j_end) {
                        let mut k = k_block;
                        let mut k_end = k_block + block_size;
                        if (k_end > size) {
                            k_end = size;
                        }

                        while (k < k_end) {
                            result[i * size + j] += a[i * size + k] * b[k * size + j];
                            k += 1;
                        }
                        j += 1;
                    }
                    i += 1;
                }
                k_block += block_size;
            }
            j_block += block_size;
        }
        i_block += block_size;
    }
}

// Calculate matrix trace (sum of diagonal elements)
fn calculateMatrixTrace(matrix: &[i32; 625], size: usize) -> i32 {
    let mut trace = 0;
    let mut i = 0;

    while (i < size) {
        trace += matrix[i * size + i];
        i += 1;
    }

    return trace;
}

// Extract submatrix for determinant calculation
fn extractSubmatrix(
    source: &[i32; 625],
    dest: &mut [i32; 16],
    source_size: usize,
    dest_size: usize,
) {
    let mut i = 0;
    while (i < dest_size) {
        let mut j = 0;
        while (j < dest_size) {
            dest[i * dest_size + j] = source[i * source_size + j];
            j += 1;
        }
        i += 1;
    }
}

// Calculate determinant of 4x4 matrix using cofactor expansion
fn calculateDeterminant4x4(matrix: [i32; 16]) -> i32 {
    let mut determinant = 0;

    // First row expansion
    let mut col = 0;
    while (col < 4) {
        let mut minor: [i32; 9] = [0; 9]; // 3x3 minor

        // Create 3x3 minor matrix
        let mut minor_i = 0;
        let mut i = 1; // Skip first row
        while (i < 4) {
            let mut minor_j = 0;
            let mut j = 0;
            while (j < 4) {
                if (j != col) {
                    minor[minor_i * 3 + minor_j] = matrix[i * 4 + j];
                    minor_j += 1;
                }
                j += 1;
            }
            minor_i += 1;
            i += 1;
        }

        // Calculate 3x3 determinant
        let minor_det = calculateDeterminant3x3(minor);

        // Add to total with appropriate sign
        if (col % 2 == 0) {
            determinant += matrix[col] * minor_det;
        } else {
            determinant -= matrix[col] * minor_det;
        }

        col += 1;
    }

    return determinant;
}

// Calculate determinant of 3x3 matrix
fn calculateDeterminant3x3(matrix: [i32; 9]) -> i32 {
    let a = matrix[0] * (matrix[4] * matrix[8] - matrix[5] * matrix[7]);
    let b = matrix[1] * (matrix[3] * matrix[8] - matrix[5] * matrix[6]);
    let c = matrix[2] * (matrix[3] * matrix[7] - matrix[4] * matrix[6]);

    return a - b + c;
}

