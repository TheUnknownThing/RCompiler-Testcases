// Comprehensive matrix calculations
fn performMatrixCalculations() -> i32 {
    let size = 25;
    let mut matrix_a: [i32; 625] = [0; 625]; // 25x25
    let mut matrix_b: [i32; 625] = [0; 625]; // 25x25
