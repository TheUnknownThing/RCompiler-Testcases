fn detectCycle(size: i32) -> bool {
    let visited = initializeVisitedArray(size);
    let rec_stack = initializeRecursionStack(size);
    let mut node = 0;
    
    while node < size {
        if !isVisited(visited, node) {
            if hasCycleDFS(node, visited, rec_stack, size) {
                return true;
            }
        }
        node += 1;
    }
    
    return false;
}

