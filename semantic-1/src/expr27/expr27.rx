fn process_array(arr: [i32; 3]) -> i32 {
    arr[0] + arr[1] + arr[2]
}

fn main() {
    let numbers = [5, 10, 15];
    let sum = process_array(numbers);
    let doubled = sum * 2;
    let result = if doubled > 50 { doubled } else { 0 }
    printInt(result);
    let final_check = result != 0;
    if final_check {
        printInt(1);
    }
}
