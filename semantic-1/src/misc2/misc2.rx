// Find maximum element in array
// Read n numbers and find the maximum value
fn main() {
    let n = readInt();
    let mut arr: [i32; 100] = [0; 100];
    let mut i = 0;
    
    // Read array elements
    while (i < n) {
        arr[i] = readInt();
        i += 1;
    }
    
    // Find maximum
    let mut max_val = arr[0];
    i = 1;
    while (i < n) {
        if (arr[i] > max_val) {
            max_val = arr[i];
        }
        i += 1;
    }
    
    printInt(max_val);
}
