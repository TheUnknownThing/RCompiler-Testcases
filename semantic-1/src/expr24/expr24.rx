fn calculate(x: i32) -> i32 {
    return x * 2 + 1;
}

fn main() {
    let input = readInt();
    let processed = calculate(input);
    let grouped = (processed + 5;
    if grouped > 0 {
        printInt(grouped);
    }
    let final_result = grouped % 10;
    printInt(final_result);
    exit(0);
}
