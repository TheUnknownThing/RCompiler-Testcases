struct Rectangle {
    width: i32,
    height: i32,
}

impl Rectangle {
    fn area(&self) -> i32 {
        self.width * self.height
    }
}

fn main() {
    let rect = Rectangle { width: 5, height: 3 };
    let area = rect.area(;
    printInt(area + 1);
    if area > 10 {
        printInt(42);
    }
}
