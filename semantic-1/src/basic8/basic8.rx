fn main() {
    let size: usize = 5;
    let table: [[i32; 5]; 5] = make_table(size);
    let unused: [[i32; 5]; 5] = table;
}

fn make_table(n: usize) -> [[i32; n]; n] {
    let mut arr: [[i32; n]; n] = [[0; n]; n];
    let mut row: usize = 0usize;
    while row < n {
        let mut col: usize = 0usize;
        while col < n {
            arr[row][col] = (row * col) as i32;
            col += 1;
        }
        row += 1;
    }
    arr
}
