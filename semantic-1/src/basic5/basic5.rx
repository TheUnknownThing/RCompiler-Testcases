fn main() {
    let mut stack: Stack = Stack::new();
    let mut i: i32 = 0;
    while (i < 10) {
        stack.push(i);
        i += 1;
    }
    while (!stack.empty()) {
        stack.pop();
    }
    exit(0);
}

struct Stack {
    data: [i32; 16],
    top: usize,
}

impl Stack {
    fn new() -> Self {
        Stack { data: [0; 16], top: 0 }
    }
}

impl Stack {
    fn push(&mut self, value: i32) {
        self.data[self.top] = value;
        self.top += 1;
    }

    fn pop(&mut self) -> i32 {
        if (self.top == 0) {
            return 0;
        }
        self.top -= 1;
        self.data[self.top]
    }
}
